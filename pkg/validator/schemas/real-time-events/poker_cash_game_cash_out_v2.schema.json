{
    "$id": "https://www.fasttrack-solutions.com/en/resources/integration/real-time-data",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Poker Cash Game Cash Out Event (POKER_CASH_GAME_CASH_OUT_V2)",
    "type": "object",
    "properties": {
      "user_id": {
        "type": "string",
        "minLength": 1
      },
      "timestamp": {
        "type": "string",
        "format": "date-time"
      },
      "status": {
        "type": "string",
        "enum": [
          "Approved",
          "Rollback"
        ]
      }
    },
    "required": [
      "currency",
      "exchange_rate",
      "status",
      "timestamp",
      "user_id"
    ]
}