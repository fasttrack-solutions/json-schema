{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Custom Event",
  "type": "object",
  "required": [
    "notification_type",
    "user_id",
    "origin",
    "timestamp",
    "data"
  ],
  "properties": {
    "notification_type": {
      "type": "string"
    },
    "user_id": {
      "type": "string"
    },
    "origin": {
      "type": "string"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "data": {
      "type": "object"
    },
    "user_agent": {
      "type": "string"
    },
    "ip_address": {
      "type": "string",
      "format": "ipv4"
    }
  }
}